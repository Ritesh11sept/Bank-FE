onst [isLoginOpen, setIsLoginOpen] = useState(false);
const [isRegisterOpen, setIsRegisterOpen] = useState(false);

// ...

<LoginModal 
  open={isLoginOpen} 
  onClose={() => setIsLoginOpen(false)}
  onRegister={() => setIsRegisterOpen(true)}
/>
<RegisterModal 
  open={isRegisterOpen} 
  onClose={() => setIsRegisterOpen(false)}
/>
